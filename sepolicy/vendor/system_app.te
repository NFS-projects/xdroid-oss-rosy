# NFSParts
allow system_app {
    sysfs_hgain
    sysfs_mgain
    sysfs_sgain
    sysfs_torch
    kcal_dev
    sysfs_vibrator
    sysfs_fpsinfo
}:file rw_file_perms;

allow system_app {
    sysfs_leds
    kcal_dev
    sysfs_vibrator
    sysfs_fpsinfo
}:dir search;

allow system_app {
    sysfs_vibrator
}:file { read write };

allow system_app {
    sysfs_fastcharge
    sysfs_fpboost
    sysfs_fsync
}:file { open read getattr write };

# zram
r_dir_file(system_app sysfs_zram)

binder_call(system_app, wificond)

allow system_app proc_pagetypeinfo:file r_file_perms;

allow system_app proc_vmallocinfo:file r_file_perms;

dontaudit system_app {
  apex_service
  system_suspend_control_service
}:service_manager find;
